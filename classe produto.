public class Produto {

    //atributos
    String NomeProduto;
    float PrecoProduto;
    int QuantidadeAdicionarProduto;
    int QuantidadePresenteNoEstoque;
    int QuantidadePossivelNoEstoque;

    //m√©todos
    public void ExibirInformacoes() {

      System.out.printf("Nome do produto:%s", NomeProduto);
      System.out.printf("%nPreco do produto:%2f", PrecoProduto);
      System.out.printf("%nQuantidade do produto no estoque:%d", QuantidadeAdicionarProduto);
      System.out.printf("%nquantidade deste produto que ja esta no estoque:%d",QuantidadePresenteNoEstoque);
      System.out.printf("%nquantidade deste produto que o estoque consegue armazenar:%d", QuantidadePossivelNoEstoque);
    
    }

}
