public class Produto {

    //atributos
    String NomeProduto;
    float PrecoProduto;
    int QuantidadeAdicionarProduto;
    int QuantidadePresenteNoEstoque;
    int QuantidadePossivelNoEstoque;

    //m√©todos
    public void ExibirInformacoes() {

      System.out.printf(%n"Nome do produto:%s", NomeProduto);
      System.out.printf(%n"Preco do produto:%2f", PrecoProduto);
      System.out.printf(%n"Quantidade do produto no estoque:%d", QuantidadeAdicionarProduto);
      System.out.printf(%n"quantidade deste produto que ja esta no estoque:%d",QuantidadePresenteNoEstoque);
      System.out.printf(%n"quantidade deste produto que o estoque consegue armazenar:%d", QuantidadePossivelNoEstoque);
    
    }

}
